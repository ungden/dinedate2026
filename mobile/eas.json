{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENV": "preview"
      }
    },
    "production": {
      "distribution": "store",
      "credentialsSource": "local",
      "ios": {
        "autoIncrement": true,
        "credentialsSource": "local"
      },
      "android": {
        "autoIncrement": true
      },
      "env": {
        "EXPO_PUBLIC_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "sai211dn@gmail.com",
        "ascAppId": "6758265568",
        "appleTeamId": "Q8A7CBYV5Z"
      },
      "android": {
        "serviceAccountKeyPath": "__SET_YOUR_KEY_PATH__",
        "track": "internal"
      }
    }
  }
}
